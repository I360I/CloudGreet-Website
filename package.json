{
  "name": "cloudgreet",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:integration": "jest --testPathPattern=integration",
    "synthetic:registration": "node scripts/monitor-registration.js",
    "seed:tollfree": "node scripts/seed-toll-free-numbers.js",
    "seed:demo": "node scripts/seed-demo-data.js",
    "test:ui": "vitest --ui",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "validate:env": "node scripts/validate-environment.js",
    "validate:env:deploy": "node scripts/validate-env.js",
    "pre-deploy": "bash scripts/pre-deploy-check.sh || node scripts/pre-deploy-check.sh",
    "validate:db": "node scripts/verify-database-schema.js",
    "generate:jwt-secret": "node scripts/generate-jwt-secret.js",
    "check:launch": "node scripts/check-launch-readiness.js",
    "prepare": "husky install",
    "setup": "node setup.js",
    "test-apis": "node test-apis.js",
    "check-trials": "node scripts/check-trials.js",
    "secrets:validate": "node scripts/validate-secrets.js validate",
    "secrets:check": "node scripts/validate-secrets.js check",
    "secrets:report": "node scripts/validate-secrets.js report",
    "secrets:rotate": "node scripts/rotate-secrets.js rotate",
    "secrets:monitor": "node scripts/monitor-secrets.js monitor",
    "secrets:security": "node scripts/monitor-secrets.js check",
    "security:audit": "npm audit --audit-level moderate",
    "security:scan": "node scripts/security-scan.js",
    "quality:check": "npm run lint:check && npm run type-check && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format",
    "pre-commit": "npm run quality:check && npm run test:unit",
    "pre-push": "npm run quality:check && npm run test:all"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@react-email/render": "^1.4.0",
    "@sentry/nextjs": "^10.22.0",
    "@supabase/supabase-js": "^2.57.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/ws": "^8.18.1",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.5.0",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "framer-motion": "^10.18.0",
    "has-flag": "^5.0.1",
    "is-number": "^7.0.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "next": "^14.2.5",
    "openai": "^4.20.1",
    "react": "^18",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18",
    "resend": "^6.1.2",
    "stripe": "^14.7.0",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.4.3",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.23.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.1.0",
    "postcss": "^8",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "optionalDependencies": {
    "@upstash/redis": "^1.35.6"
  }
}
